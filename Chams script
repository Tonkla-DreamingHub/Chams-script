	table.insert(Connections, Player:GetPropertyChangedSignal("TeamColor"):Connect(function()
		UpdateFillColor(Player, Highlighter)
	end))

	-- เธฐยเธฐเธ‘เธฑโฌเธฐเธเธฐเธ‘เธฐเธเธฑยเธฑยเธฐเธเธฐเธ เธฐเธเธฐเธ—เธฐเธเธฐเธ•เธฐเธเธฐเธ•เธฐเธเธฐเธเธฑย เธฐเธ—เธฐเธ”เธฐเธเธฑโฌเธฐเธเธฐเธ’เธฑยเธฑย
	table.insert(Connections, Humanoid:GetPropertyChangedSignal("Health"):Connect(function()
		if Humanoid.Health <= 0 then
			Disconnect(Highlighter, Connections)
		end
	end))

	-- เธฐยเธฑยเธฐเธเธฐเธเธฑยเธฑยเธฐเธ•เธฐเธเธฐเธเธฐเธ• เธฐเธเธฐเธเธฐเธ”เธฑยเธฐเธ’เธฐเธ•เธฑยเธฐเธเธฐเธ เธฐเธเธฑโฌเธฐเธ เธฑยเธฐเธ”เธฐเธเธฐเธเธฐเธ•เธฐเธเธฐเธเธฐเธ เธฐเธเธฐเธ•เธฑโฌเธฑยเธฐเธเธฐเธเธฐเธเธฐเธ–เธฐเธ
	Character.AncestryChanged:Connect(function(_, parent)
		if not parent then
			Disconnect(Highlighter, Connections)
		end
	end)
end

-- เธฐเธเธฑยเธฐเธเธฐเธเธฑยเธฐเธเธฑย เธฐเธเธฑโฌเธฐเธเธฐเธเธฐเธ•เธฐเธเธฐเธ•เธฐเธเธฐเธเธฑย เธฐเธเธฐเธเธฐเธ”เธฑยเธฐเธ’เธฐเธ•เธฑยเธฐเธเธฐเธ เธฐเธ เธฐเธเธฐเธ“เธฑโฌเธฐเธเธฐเธเธฑย
local function ApplyHighlight(Player)
	-- เธฐยเธฐเธเธฐเธ”เธฑยเธฐเธ’เธฐเธ•เธฑยเธฐเธเธฐเธ เธฐเธเธฐเธ“เธฑโฌเธฐเธเธฐเธเธฐเธ, เธฐเธ•เธฑยเธฐเธเธฐเธ เธฑย เธฐเธเธฐเธ•เธฐเธ“เธฐเธ เธฑยเธฐเธ–เธฐเธ• เธฐเธ•เธฑยเธฑยเธฑย เธฐเธเธฐเธ•เธฑโฌเธฑยเธฐเธเธฐเธเธฐเธเธฐเธ–
	if Player.Character then
		SetupCharacter(Player, Player.Character)
	end

	-- เธฐยเธฐเธเธฐเธ”เธฑยเธฐเธ’เธฐเธ•เธฑยเธฐเธเธฐเธ เธฐเธเธฐเธเธฐเธ’เธฐเธเธฐเธ“เธฐเธ เธฐเธเธฐเธ•เธฑโฌเธฑยเธฐเธเธฐเธเธฐเธเธฐเธ–เธฐเธ, เธฐเธเธฐเธเธฐเธ“เธฐเธ”เธฐเธ เธฐเธเธฐเธ เธฑยเธฐเธเธฐเธ—เธฐเธ”เธฐเธเธฐเธ•เธฑยเธฑยเธฑย
	Player.CharacterAdded:Connect(function(Character)
		SetupCharacter(Player, Character)
	end)
end

-- เธฐยเธฑโฌเธฐเธเธฐเธเธฐเธ•เธฐเธเธฐเธ•เธฐเธเธฐเธเธฐเธ• เธฐเธเธฐเธเธฐเธ”เธฑยเธฐเธ’เธฐเธ•เธฑยเธฐเธเธฐเธ เธฐเธเธฐเธ เธฐเธ’เธฑยเธฐเธ•เธฐเธ เธฑยเธฑยเธฑยเธฐเธ•เธฑยเธฑยเธฐเธ’เธฑยเธฑยเธฑยเธฐเธเธฐเธ เธฐเธเธฐเธ“เธฑโฌเธฐเธเธฐเธเธฐเธเธฐเธ
for _, Player in ipairs(Players:GetPlayers()) do
	ApplyHighlight(Player)
end

-- เธฐยเธฑโฌเธฐเธเธฐเธเธฐเธ•เธฐเธเธฐเธ•เธฐเธเธฐเธเธฐเธ• เธฐเธเธฐเธเธฐเธ”เธฑยเธฐเธ’เธฐเธ•เธฑยเธฐเธเธฐเธ เธฐเธ เธฐเธเธฐเธเธฐเธ’เธฑยเธฐเธ เธฐเธเธฐเธ“เธฑโฌเธฐเธเธฐเธเธฐเธเธฐเธ
Players.PlayerAdded:Connect(ApplyHighlight)
end)
end)
